                                             action="#{pageFlowScope.navigateBean.settlementdashboardflow}" id="l12"/>
                                    <af:link text="Fixed Assets Transactions"   rendered="#{applicationScope.get('pageList').contains('ALL_PAGE') or  applicationScope.get('pageList').contains('FIXED_ASSETS')}"
                                             action="#{pageFlowScope.navigateBean.sgsfixedAssetsTxnflow}" id="fat01"/>
