                                             action="#{pageFlowScope.navigateBean.sgsgstInquiryflow}" id="l5"
                                             immediate="true"/>
                                    <af:link text="Value Added Tax(VAT) Setup"   rendered="#{applicationScope.get('pageList').contains('ALL_PAGE')  or  applicationScope.get('pageList').contains('VAT')}"
                                             action="#{pageFlowScope.navigateBean.sgsvatInquiryflow}" id="l6"
                                             immediate="true"/>
                                    <af:link text="Withholding Tax (WHT)/ TDS Setup"   rendered="#{applicationScope.get('pageList').contains('ALL_PAGE')  or  applicationScope.get('pageList').contains('WHT')}"
                                             action="#{pageFlowScope.navigateBean.sgstdswhtInquiryflow}" id="l8"
                                             immediate="true"/>
