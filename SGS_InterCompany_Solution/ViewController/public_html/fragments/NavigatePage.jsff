                                             action="#{pageFlowScope.navigateBean.sgsnettingflow}" id="l2"/>
                                </af:panelList>
                            </af:showDetailItem>
                            <af:showDetailItem id="Process" text="Process" rendered="#{applicationScope.get('pageList').contains('ALL_PAGE')}">
                                <af:panelList id="pl3">
                                    <!--<af:link text="link 1" id="l2"/>-->
                                    <af:link text="Allocation Run"
                                             action="#{pageFlowScope.navigateBean.sgsAllocationRunflow}" id="arp01"/>
                                </af:panelList>
                            </af:showDetailItem>
                            <af:showDetailItem id="Reports" text="Reports" rendered="#{applicationScope.get('pageList').contains('ALL_PAGE')}"/>
