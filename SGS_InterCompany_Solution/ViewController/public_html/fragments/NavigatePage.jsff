                                             action="#{pageFlowScope.navigateBean.sgstpaInquiryflow}" immediate="true"/>
                                </af:panelList>
                            </af:showDetailItem>
                            <af:showDetailItem id="Transaction" text="Transactional Data"  rendered="#{applicationScope.get('pageList').contains('ALL_PAGE')  or  applicationScope.get('pageList').contains('TXN_DATA')}">
                                <af:panelList id="pl2">
                 
