<?xml version='1.0' encoding='UTF-8'?>
<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
                xmlns:f="http://java.sun.com/jsf/core">

  <af:panelStretchLayout id="psl1">
    <f:facet name="start"/>
    <f:facet name="end"/>
    <f:facet name="top"/>
    <f:facet name="bottom"/>
    <f:facet name="center">
      <af:panelBox text="Statistical Data" id="pb1" inlineStyle="margin-top: 1px;">
        <f:facet name="toolbar"/>
        <af:panelGroupLayout layout="vertical" id="pgl1">
         
            <af:query id="qryId1" headerText="Search" disclosed="true"
                      value="#{bindings.SgsStatisticalDataVOCriteriaQuery.queryDescriptor}"
                      model="#{bindings.SgsStatisticalDataVOCriteriaQuery.queryModel}"
                      queryListener="#{bindings.SgsStatisticalDataVOCriteriaQuery.processQuery}"
                      queryOperationListener="#{bindings.SgsStatisticalDataVOCriteriaQuery.processQueryOperation}"
                      saveQueryMode="hidden" rows="3" maxColumns="3"/>
       
        </af:panelGroupLayout>
        <af:panelCollection id="pc1" styleClass="AFStretchWidth">
          <f:facet name="menus"/>
          <f:facet name="toolbar">
            <af:toolbar id="t2">
              <af:button text="Copy From Previous Month" id="b3"/>
              <af:button text="Upload Data" id="b5"/>
              <!--<af:button text="Delete Data" id="b6"/>-->
              <af:button text="Submit For Approval" id="b7"/>
              <af:button  actionListener="#{viewScope.ActionEventsBean.onStatisticalDataSave}" text="Save" id="b4"/>
              <af:button id="b2" shortDesc="Delete" icon="/resources/icons/delete.png"
                         actionListener="#{viewScope.ActionEventsBean.onDeleteStatisticalData}"/>
              <af:button  id="b1" action="ADD" icon="/resources/icons/plus.png" shortDesc="Create/Add Statistical Data"
                actionListener="#{bindings.CreateStatisticalData.execute}"/>
              
         
            </af:toolbar>
          </f:facet>
          <f:facet name="statusbar"/>
