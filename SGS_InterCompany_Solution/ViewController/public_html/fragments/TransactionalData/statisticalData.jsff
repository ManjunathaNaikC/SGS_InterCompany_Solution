                    rows="#{bindings.SgsStatisticalDataVO1.rangeSize}"
                    emptyText="#{bindings.SgsStatisticalDataVO1.viewable ? 'No data to display.' : 'Access Denied.'}"
                    rowBandingInterval="0"
                    selectedRowKeys="#{bindings.SgsStatisticalDataVO1.collectionModel.selectedRow}"
                    selectionListener="#{bindings.SgsStatisticalDataVO1.collectionModel.makeCurrent}"
                    rowSelection="single" fetchSize="#{bindings.SgsStatisticalDataVO1.rangeSize}"
                    filterModel="#{bindings.SgsStatisticalDataVO1Query.queryDescriptor}" filterVisible="true"
                    queryListener="#{bindings.SgsStatisticalDataVO1Query.processQuery}" varStatus="vs" id="t1"
                    styleClass="AFStretchWidth">
            <af:column sortProperty="#{bindings.SgsStatisticalDataVO1.hints.StatisticalDataCategory.name}"
                       filterable="true"
                       headerText="#{bindings.SgsStatisticalDataVO1.hints.StatisticalDataCategory.label}" id="c1">
              <af:inputText value="#{row.bindings.StatisticalDataCategory.inputValue}"
                            label="#{bindings.SgsStatisticalDataVO1.hints.StatisticalDataCategory.label}"
                            required="#{bindings.SgsStatisticalDataVO1.hints.StatisticalDataCategory.mandatory}"
                            columns="#{bindings.SgsStatisticalDataVO1.hints.StatisticalDataCategory.displayWidth}"
                            maximumLength="#{bindings.SgsStatisticalDataVO1.hints.StatisticalDataCategory.precision}"
                            shortDesc="#{bindings.SgsStatisticalDataVO1.hints.StatisticalDataCategory.tooltip}"
                            id="it1">
                <f:validator binding="#{row.bindings.StatisticalDataCategory.validator}"/>
              </af:inputText>
            </af:column>
            <af:column sortProperty="#{bindings.SgsStatisticalDataVO1.hints.ToBusinessUnit.name}" filterable="true"
