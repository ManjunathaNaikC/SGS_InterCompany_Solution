<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="createsettlement_flow">
    <default-activity>createSettlement</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>ActionEventsBean</managed-bean-name>
      <managed-bean-class>com.sgs.ics.view.ActionEventsBean</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="createSettlement">
      <page>/fragments/TransactionalData/createSettlement.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
